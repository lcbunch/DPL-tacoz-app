<h1>Edit Menu Item</h1>
<%= form_for [:admin, @menu_item] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :vegetarian %>
  <%= f.check_box :vegetarian %>

  <%= f.label :picture %>
  <%= f.file_field :picture %>

  <%= f.submit %>
<% end %>
